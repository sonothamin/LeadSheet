{
    "version": 2,
    "functions": {
      "index.js": {
        "runtime": "vercel-node@20.x"  // Use the Node.js runtime version you need
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ]
  }
  