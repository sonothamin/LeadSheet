{
    "version": 2,
    "functions": {
      "index.js": {
        "runtime": "@vercel/node"
      },
      "print.js": {
        "runtime": "@vercel/node"
      }
    },
    "routes": [
      {
        "src": "/api/lyrics",
        "dest": "/index.js"
      },
      {
        "src": "/print",
        "dest": "/print.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }
  