{
    "version": 2,
    "functions": {
      "index.js": {
        "runtime": "vercel-node@20.x"
      }
    },
    "routes": [
        
        {
        "src": "/",
        "dest": "/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "builds": [
      {
        "src": "public/**/*",
        "use": "@vercel/static-build"
      }
    ]
  }
  